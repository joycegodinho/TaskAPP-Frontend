ne":96,"column":17},"source":"components/UserForm.js","original":{"line":58,"column":4}},{"generated":{"line":97,"column":0},"source":"components/UserForm.js","original":{"line":88,"column":1}},{"generated":{"line":97,"column":1},"source":"components/UserForm.js","original":{"line":51,"column":0}},{"name":"UserForm","generated":{"line":99,"column":15},"source":"components/UserForm.js","original":{"line":90,"column":15}}],"sources":{"components/UserForm.js":"import React, {  useState } from 'react';\r\nimport styled from 'styled-components';\r\n\r\nimport Button from './Button';\r\n\r\nconst Wrapper = styled.div`\r\n    border-bottom: 1px solid #B8B8B9;\r\n    border-top: 1px solid #B8B8B9;\r\n    border-left: 1px solid #B8B8B9;\r\n    border-right: 1px solid #B8B8B9;\r\n    box-shadow: 0 0 4px 0 rgba(0, 0, 0, 0.25);\r\n    border-radius: 10px;\r\n    background-image: #FFFFFF;\r\n    color: #000000;\r\n    max-width: 500px;\r\n    height: auto;\r\n    max-height: 400px;\r\n    padding: 1em;\r\n    margin: auto;\r\n\r\n    \r\n`;\r\n\r\nconst Form = styled.form`\r\n    \r\n    label,\r\n    input {\r\n        display: block;\r\n        line-height: 2em;\r\n    }\r\n    input {\r\n        width: 100%;\r\n        margin-bottom: 1em;\r\n    }\r\n`;\r\nconst Input = styled.input`\r\n    border-radius: 5px;\r\n    border-bottom: 1px solid #B8B8B9;\r\n    border-top: 1px solid #B8B8B9;\r\n    border-left: 1px solid #B8B8B9;\r\n    border-right: 1px solid #B8B8B9;\r\n    box-shadow: 0 0 2px 0 rgba(0, 0, 0, 0.25);\r\n`\r\n\r\nconst StyledButtom = styled.div`\r\n    margin-left:195px;\r\n    margin-top: 20px;\r\n\r\n`\r\n\r\nconst UserForm = props => {\r\n\r\n    const [values, setValues] = useState();\r\n    const onChange = event => {\r\n        setValues({...values, [event.target.name]:event.target.value});\r\n    };\r\n\r\n    return (\r\n        <Wrapper>\r\n            {props.formType === 'signup' ? <h2>Sign Up</h2> : <h2>Sign In</h2>}\r\n            <Form onSubmit={e => {\r\n                e.preventDefault();\r\n                props.action({variables: {...values}})\r\n            }}>\r\n        \r\n                {props.formType === 'signup' && (\r\n                    <React.Fragment>\r\n\r\n                        <label htmlFor=\"username\">Username:</label>\r\n                        <Input required type=\"text\" id=\"username\" name=\"username\" placeholder=\"username\" onChange={onChange}/>\r\n                    </React.Fragment>\r\n                )}\r\n                <label htmlFor=\"email\">Email:</label>\r\n                <Input required type=\"email\" id=\"email\" name=\"email\" placeholder=\"Email\" onChange={onChange}/>\r\n                        \r\n                <label htmlFor=\"password\">Password:</label>\r\n                <Input required type=\"password\" id=\"password\" name=\"password\" placeholder=\"Password\" onChange={onChange}/>\r\n\r\n                <StyledButtom>\r\n                    <Button type=\"submit\">Submit</Button>\r\n                </StyledButtom>\r\n\r\n                \r\n\r\n            </Form>\r\n        </Wrapper>\r\n    );\r\n};\r\n\r\nexport default UserForm;"},"lineCount":null}},"error":null,"hash":"92c5208e044a742ca1aa616d5496c8f2","cacheData":{"env":{}}}