om './signin';\r\nimport NewTask from './new'\r\nimport EditTask from './edit'\r\n\r\nconst IS_LOGGED_IN = gql`\r\n    {\r\n        isLoggedIn @client\r\n    }\r\n`;\r\n\r\nconst Pages = () => {\r\n    return (\r\n        <Router>  \r\n            <Layout>\r\n                <PrivateRoute exact path=\"/\" component={Home} />\r\n                <Route path=\"/task/:id\" component={SingleTask} /> \r\n                <Route path=\"/new\" component={NewTask} />\r\n                <PrivateRoute path=\"/edit/:id\" component={EditTask} />\r\n                <Route path=\"/signup\" component={SignUp} />\r\n                <Route path=\"/signin\" component={SignIn} />\r\n            </Layout>     \r\n        </Router>\r\n    );\r\n};\r\n\r\nconst PrivateRoute = ({ component: Component, ...rest}) => {\r\n    const { loading, error, data } = useQuery(IS_LOGGED_IN);\r\n    if(loading) return <p>Loading...</p>\r\n    if(error) return <p>Error!</p>\r\n    return (\r\n        <Route {...rest} render={props => data.isLoggedIn === true ? (\r\n            <Component {...props} /> ) : (\r\n            <Redirect to = {{ pathname: 'signin', state :{from: props.location}}} />\r\n            )} />\r\n    );\r\n};\r\n\r\nexport default Pages;"},"lineCount":null}},"error":null,"hash":"38bff76ee837c184844eb0fa35dad134","cacheData":{"env":{}}}