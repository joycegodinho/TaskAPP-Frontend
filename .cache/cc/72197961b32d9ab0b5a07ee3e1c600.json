:{"components/Task.js":"import React from 'react';\r\nimport ReactMarkdown from 'react-markdown'\r\n\r\nimport { format } from 'date-fns';\r\nimport styled from 'styled-components';\r\n\r\nconst StyledTask = styled.article`\r\n    max-width: 800px;\r\n    margin: 0 auto;\r\n    color: #000000;\r\n\r\n`;\r\nconst MetaData = styled.div`\r\n    @media (min-width: 500px) {\r\n        display: flex;\r\n        align-items: top;\r\n    }\r\n`;\r\nconst MetaInfo = styled.div`\r\n    padding-right: 1em;\r\n    padding-left: 0em\r\n`;\r\n\r\n\r\nconst Task = ({ task }) => {\r\n    return (\r\n        <StyledTask>\r\n            <MetaData>\r\n                <MetaInfo>\r\n                <em>by</em> {task.author.username} <br />\r\n                <em>created at</em> {format(new Date(task.createdAt), 'MM dd yyyy')} <br />\r\n                <em>updated at</em> {format(new Date(task.updatedAt), 'MM dd yyyy')} <br />\r\n                <em>completed</em> {String(task.completed)}\r\n                \r\n                </MetaInfo>\r\n            </MetaData>\r\n            <ReactMarkdown source={task.content} />\r\n            \r\n        </StyledTask>\r\n    )\r\n}\r\n\r\nexport default Task;"},"lineCount":null}},"error":null,"hash":"75651e60151aa0b9d20c9ae7e67107f1","cacheData":{"env":{}}}